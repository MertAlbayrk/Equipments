

@{
    ViewBag.Title = "Printers";
}
<section>
    <div class="box box-primary">
        <div class="box-header">
            <!-- tools box -->
            <div class="pull-right box-tools">
                <button class="btn btn-primary btn-sm pull-right" data-widget='collapse' data-toggle="tooltip" title="Collapse" style="margin-right: 5px;"><i class="fa fa-minus"></i></button>
            </div><!-- /. tools -->
            <i class="fa fa-user"></i>
            <h3 class="box-title">
                Printers Search
            </h3>
        </div>
        <div class="box-body no-padding">
            <div id="world-map" style="height: 5px;"></div>

            <div class="form-group row">
                <div class="col-xs-3">
                    <label>Demirbas</label>
                    <input class="form-control" id="txt_Demirbaş Kodu" type="text">
                </div>
                <div class="col-xs-3">
                    <label>Marka</label>
                    <input class="form-control" id="txt_Marka" type="text">
                </div>
                <div class="col-xs-3">
                    <label>IP</label>
                    <input class="form-control" id="txt_IP" type="text">
                </div>
                <div class="col-xs-3">
                    <label>Temel Özellikler</label>
                    <input class="form-control" id="txt_Temel Özellikler" type="text">
                </div>
                <div class="col-xs-3">
                    <label>Network Name</label>
                    <input class="form-control" id="txt_Networ Name" type="text">
                </div>
                <div class="col-xs-3">
                    <label>Location</label>
                    <input class="form-control" id="txt_Location" type="text">
                </div>
                <div class="col-xs-3">
                    <label>Envanter Number</label>
                    <input class="form-control" id="txt_Envanter Number" type="text">
                </div>
                <div class="col-xs-3">
                    <label>Invoice Contract</label>
                    <input class="form-control" id="Invoice Contract" type="text">
                </div>
                <div class="col-xs-3">
                    <label>Status</label>
                    <select id="cmb_role" class="form-control">
                        <option value="Selection">Selection</option>
                        <option value="Working">Working</option>
                        <option value="Broken">Broken</option>
                        <option value="Not In UsE">Not In Use</option>
                    </select>
                </div>
                <div class="col-xs-3">
                    <label>Purchase Date</label>
                    <input class="form-control" id="txt_PurchaseDate" type="date">
                </div>


            </div><!-- /.box-body-->
            <div class="box-footer">
                <button class="btn btn-info" onclick="Save()"><i class="fa fa-save"></i>  Bilgileri Kaydet</button>
                <button class="btn btn-warning" onclick="Temizle()"><i class="fa fa-repeat"></i>  Temizle</button>
            </div>
        </div>


</section>
<br />
<section>
    <div class="box box-danger">
        <div class="box-header">
            <div class="box-header">
                <h3 class="box-title"><i class="fa fa-wheelchair"></i><strong>Printers Search İnformation</strong></h3>
                <div style="float:right;">
                    <select id="pageSize" class="btn btn-xs btn-default" title="Total User Number" style="float:right;">
                        <option>15</option>
                        <option>25</option>
                        <option>50</option>
                    </select>
                    <div style="clear: both;"></div>
                </div>
            </div>
        </div>
        <div class="box-body no-padding">
            <div id="world-map" style="height: 5px;"></div>
            <div class="box-body table-responsive no-padding" style="overflow-x:scroll;">
                <table class="table table-bordered table-striped table-hover">
                    <thead>
                        <tr>
                            <th>Demirbas</th>
                            <th>Marka</th>
                            <th>IP Number</th>
                            <th>Temel Özellikler</th>
                            <th>Network Name</th>
                            <th>Location</th>
                            <th>Envanter Number</th>
                            <th>Invoice Contract</th>
                            <th>Status</th>
                            <th>Purchase Date</th>


                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
            <div class="box-footer">

            </div>
        </div>

    </div>
</section>







